{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "es2019"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "experimentalDecorators": true,
    "types": ["cypress"]
  }
}
